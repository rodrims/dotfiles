alias cls='clear'
alias dirs='dirs -p -v'
alias df='df -h --total'
alias fknbt='sudo modprobe -r btusb; sudo modprobe btusb'
alias fknwifi='sudo service network-manager restart'
alias gcc='gcc -g -std=c99'
alias g++='g++ -g -std=c++11'
alias isinst='apt list --installed | grep'
alias l='ls -CF'
alias la='ls -Al'
alias linst='apt list --installed'
alias ll='ls -AlF'
alias pls='sudo $(fc -ln -1)'
